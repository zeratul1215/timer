{
  "name": "link-monorepo-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "prepare": "husky",
    "build:all": "pnpm -r --parallel build",
    "dev:admin": "pnpm admin dev",
    "build:admin": "pnpm admin build",
    "admin": "pnpm --filter=@link/admin",
    "lint": "eslint packages --fix --max-warnings=0",
    "format": "prettier --write --list-different \"./**/*.{js,jsx,ts,tsx,css,md,json,yaml}\" ",
    "typecheck": "tsc --noEmit --project tsconfig.app.json"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.27.1",
    "@commitlint/cli": "^19.4.1",
    "@commitlint/config-conventional": "^19.4.1",
    "@csstools/postcss-global-data": "^3.0.0",
    "@eslint/js": "^9.25.0",
    "@types/node": "^22.15.19",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.5.0",
    "autoprefixer": "^10.4.20",
    "husky": "^9.1.5",
    "lint-staged": "^15.2.9",
    "postcss": "^8.4.24",
    "postcss-custom-media": "^11.0.1",
    "postcss-nested": "^6.0.1",
    "prettier": "^3.5.3",
    "vite-plugin-svgr": "^4.2.0",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5"
  }
}
